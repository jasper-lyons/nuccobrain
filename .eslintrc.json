{
  "parser": "babel-eslint",
  "env": {
      "browser": true,
      "es6": true,
      "jquery": true
  },
  "settings": {
      "ecmascript": 6,
      "jsx": true
//   },
  "parserOptions": {
      "ecmaVersion": 2017,
      "ecmaFeatures": {
          "experimentalObjectRestSpread": true,
          "jsx": true
      },
      "sourceType": "module"
  },
  "extends": "airbnb-base",
  "plugins": [
  "react",
  "import"
  ],
  "rules": {
	  "linebreak-style": 0,
      "indent": [2, "tab"],
      "no-tabs": 0,
      "react/jsx-indent": 0,
      "react/jsx-indent-props": [1, "tab"],
      "react/jsx-wrap-multilines": 0,
      "react/jsx-filename-extension": 0,
      "jsx-a11y/anchor-is-valid": [0, {
          "components": ["Link"]
      }],
      "prefer-destructuring": ["error", {
        "VariableDeclarator": {
          "array": true,
          "object": true
        },
        "AssignmentExpression": {
          "array": true,
          "object": true
        }
      }, {
        "enforceForRenamedProperties": false
      }],
      "react/forbid-prop-types": 0
  }
}
